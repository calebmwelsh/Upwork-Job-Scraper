            "title": "🔍 Search Query",
            "type": "string",
            "example": "Workflow Automation",
            "description": "The primary search term for jobs (e.g., 'Workflow Automation').",
            "editor": "textfield"
        },
        "limit": {
            "title": "♾️ Limit",
            "type": "integer",
            "example": 50,
            "description": "Maximum number of jobs to fetch.",
            "default": 50,
            "editor": "number"
