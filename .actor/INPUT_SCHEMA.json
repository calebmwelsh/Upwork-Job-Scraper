            "example": "user@example.com",
            "description": "Your Upwork username.",
            "editor": "textfield"
        },
        "password": {
            "title": "🤐 Password",
            "type": "string",
            "example": "yourPassword123!",
            "description": "Your Upwork password.",
            "editor": "textfield",
            "isSecret": true
